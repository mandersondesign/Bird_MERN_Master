{
  "name": "bird-backend-api",
  "version": "1.0.0",
  "description": "API service on TypeScript",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "npx eslint --ext=ts,tsx src",
    "build": "npx tsc",
    "dev": "TS_NODE_LOG_ERROR=true TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon --watch './**/*.ts' --exec ts-node ./src/server.ts",
    "start": "NODE_ENV=production npx node ./dist/server.js",
    "cron": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true npx ts-node ./src/cronDaemon.ts",
    "strava": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true npx ts-node ./dev/strava.ts",
    "prod": "npm run build && npm run start",
    "migrate:up": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true npx ts-node ./node_modules/typeorm/cli.js migration:run",
    "//": "downgrade last migration",
    "migrate:down": "npx ts-node ./node_modules/typeorm/cli.js migration:revert",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@sentry/node": "^5.11.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.0",
    "@types/hapi__joi": "^17.1.7",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^10.17.60",
    "@types/nodemailer": "^4.6.8",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "await-to-js": "^2.1.1",
    "aws-sdk": "^2.929.0",
    "axios": "^0.20.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.0",
    "email-validator": "^2.0.4",
    "envalid": "^5.0.0",
    "export-to-csv": "^0.2.1",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "googleapis": "^39.2.0",
    "http-status-codes": "^1.4.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.19",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "nodemailer": "^5.1.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "number-to-words": "^1.2.4",
    "password-validator": "^5.1.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "sendbird-nodejs": "github:ConsiderItDone/sendbird-nodejs",
    "socket.io": "4.2.0",
    "sqlite3": "^5.0.2",
    "strava_api_v3": "github:blineberry/StravaAPIv3-js",
    "stripe": "^8.154.0",
    "ts-node": "^8.10.2",
    "twilio": "^3.63.1",
    "typeorm": "^0.2.34",
    "typescript": "3.9.9"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^24.9.0",
    "mockdate": "^2.0.5",
    "ts-jest": "^24.3.0",
    "typeorm-model-generator": "^0.4.5"
  }
}
